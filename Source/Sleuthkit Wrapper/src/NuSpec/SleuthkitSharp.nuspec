<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>SleuthkitSharp</id>
    <version>2.0.0</version>
    <authors>Sandun Weerasinghe, Sathyendra Balaretnaraja, Josef Eklann</authors>
    <owners>Safer Society Group</owners>
    <dependencies />
    <licenseUrl>http://www.safersociety.com</licenseUrl>
    <projectUrl>http://www.safersociety.com</projectUrl>
    <iconUrl>http://www.safersociety.com</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A wrapper of sleuthkit for C#</description>
    <releaseNotes>
      1.3.1 Reworked nuget package to better handle references to different platforms x86/x64.
      1.4.0 Added logic to detect deleted / overwritten files.
      2.0.0 Updated to sleuthkit 4.3.1, should support vhdi and vmdk. Dropped x86 support.
    </releaseNotes>
    <copyright></copyright>
  </metadata>
  <files>
    <file src="..\bin\x86\Release\sleuthkit-sharp.*" target="lib\net45\" />
    <file src="..\bin\x64\Release\sleuthkit-sharp.*" target="build\x64" />
        
    <file src="..\..\..\..\Lib\x64\ewf.net.dll" target="CopyToOutput\x64" />
    <file src="..\..\..\..\Lib\x64\libewf.dll" target="CopyToOutput\x64" />
    <file src="..\..\..\..\Lib\x64\libvhdi.dll" target="CopyToOutput\x64" />
    <file src="..\..\..\..\Lib\x64\libvmdk.dll" target="CopyToOutput\x64" />
    <file src="..\..\..\..\Lib\x64\libtsk4.dll" target="CopyToOutput\x64" />
    <file src="..\..\..\..\Lib\x64\zlib.dll" target="CopyToOutput\x64" />
  
    <file src="SleuthkitSharp.targets" target="build" />
  </files>
</package>